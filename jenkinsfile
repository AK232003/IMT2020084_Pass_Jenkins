pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'https://github.com/AK232003/IMT2020084_Jenkins_Assignment.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x Limited_Subtraction.py"
                sh "./Limited_Subtraction.py"
            }
        }
        stage('Test Code Passed') {
            steps {
                sh "chmod u+x Unit_test1.py"
                sh "./Unit_test1.py"
            }
        }
    } 
}
